{
    "manifestVersion": 1,
    "id": "build-release-task",
    "name": "Provenance Generator",
    "version": "0.31.0",
    "publisher": "gattjoe",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Provenance Generator",
